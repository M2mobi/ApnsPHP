{
	"name": "m2mobi/apns-php",
	"type": "library",
	"description": "Apple Push Notification & Feedback Provider",
	"keywords": ["APNS", "apple", "push", "notification"],
	"homepage": "https://github.com/M2mobi/ApnsPHP",
	"license": "BSD-3-Clause",
	"require": {
		"php": ">=7.4",
		"ext-curl": ">=7.4",
		"ext-json": ">=7.4",
		"ext-mbstring": ">=7.4",
		"lib-curl": ">=7.33.0",
		"lib-openssl": "*",
		"psr/log": ">=1.1",
		"lcobucci/jwt": "~4.1.5 || ^5.1"
	},
	"require-dev": {
		"phpunit/phpunit": ">=9.0 <9.6",
		"lunr/halo": "~0.7.0"
	},
	"config": {
		"optimize-autoloader": true
	},
	"autoload": {
        "psr-4": {"ApnsPHP\\": "ApnsPHP"}
    }
}
